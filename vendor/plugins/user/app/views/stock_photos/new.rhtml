<% form_for :stock_photo, @photo, :url => {:action => 'create'}, :html => {:multipart => true}  do |f| %>
  <%= f.file_field :data %>
  <select name="stock_type">
    <%= options_for_select(::StockPhotosController.stock_types.collect { |t| [t.to_s, t.to_s] }) %>
  </select>
  <%= submit_tag "Save" %>
<% end %>
